{
    "name": "ar_garage",
    "description": "Simple Biling software for micro entreprise",
    "version": "0.1.18",
    "private": true,
    "author": "ARNAUDIN RÃ©my",
    "proxy": "http://localhost:4000",
    "homepage": ".",
    "build": {
        "asar": true,
        "directories": {
            "output": "out"
        },
        "extraMetadata": {
            "main": "main.js"
        },
        "win": {
            "target": "nsis",
            "icon": "public/favicon.ico",
            "publish": [
                {
                    "provider": "github",
                    "repo": "AR_Garage",
                    "owner": "remyar",
                    "releaseType": "draft"
                }
            ],
            "artifactName": "AR_Garage-Setup.${ext}",
            "files": [
                "!out/*",
                "!webpack*",
                "!src/*",
                "!*.log",
                "!node_modules/*",
                "!dev-app-update.yml",
                "!.vscode/*",
                "!.github/*",
                "!*.md",
                "!public/*"
            ]
        },
        "nsis": {
            "include": "./installer.nsh"
        }
    },
    "dependencies": {
        "@craco/craco": "^7.0.0",
        "@electron/remote": "^2.0.9",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.11.0",
        "@mui/lab": "^5.0.0-alpha.119",
        "@mui/material": "^5.11.8",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "cli-progress": "^3.11.2",
        "dotenv": "^16.0.3",
        "electron-fetch": "^1.9.1",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.4.8",
        "electron-logger": "0.0.3",
        "electron-store": "^8.1.0",
        "electron-updater": "^5.3.0",
        "formik": "^2.2.9",
        "html2json": "^1.0.2",
        "isomorphic-fetch": "^3.0.0",
        "jimp": "^0.16.2",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.5.28",
        "lodash": "^4.17.21",
        "node-fetch": "^3.3.0",
        "node-html-parser": "^6.1.4",
        "node-tesseract-ocr": "^2.2.1",
        "notistack": "^2.0.8",
        "react": "^18.2.0",
        "react-alice-carousel": "^2.7.0",
        "react-big-calendar": "^1.6.4",
        "react-datepicker": "^4.10.0",
        "react-dom": "^18.2.0",
        "react-input-mask": "^2.0.4",
        "react-intl": "^6.2.8",
        "react-router-dom": "^6.8.1",
        "react-scripts": "^5.0.1",
        "remove-accents": "^0.4.4",
        "sqlite3": "^5.1.4",
        "string-sanitizer": "^2.0.2",
        "string-similarity": "^4.0.4",
        "yup": "^1.0.0"
    },
    "devDependencies": {
        "electron": "^23.0.0",
        "electron-builder": "^23.6.0"
    },
    "scripts": {
        "start:electron": "electron main.js",
        "start": "craco start",
        "build": "craco build",
        "dist": "electron-builder",
        "test": "craco  test"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
